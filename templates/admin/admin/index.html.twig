{% extends 'base2.html.twig' %}
{% block body %}
    <div class="breadcrumbs">
        <div class="container">
            <div class="row">
                <div class="col-12">
                    <div class="bread-inner">
                        <ul class="bread-list">
                            <li><a href="index1.html">Acceuil<i class="ti-arrow-right"></i></a></li>
                            <li class="active"><a href="blog-single.html">Administration</a></li>
                        </ul>
                    </div>
                </div>
            </div>
        </div>
    </div><br>
    <div class="container">
        <div class="row">
            <div class="col-md-12">
                <div class="card">
                    <div class="card-body">
                        <div class="d-md-flex align-items-center">
                            <div>
                                <h4 class="card-title">Bilan</h4>
                                <h5 class="card-subtitle">Bilan Journalier</h5>
                            </div>
                        </div>
                        <div class="row">
